# Place this file in your /etc/yum.repos.d/ directory

[epel-qpid-cpp]
name=Qpid packages for epel-6
baseurl=http://repos.fedorapeople.org/repos/mcpierce/qpid-cpp/epel-$releasever/$basearch/
enabled=1
skip_if_unavailable=1
gpgcheck=0

[epel-qpid-cpp-source]
name=Qpid packages for epel-6 - Source
baseurl=http://repos.fedorapeople.org/repos/mcpierce/qpid-cpp/epel-$releasever/SRPMS
enabled=0
skip_if_unavailable=1
gpgcheck=0
